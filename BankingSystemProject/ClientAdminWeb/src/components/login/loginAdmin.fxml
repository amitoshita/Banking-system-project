            <ImageView fx:id="logoImage" fitHeight="78.0" fitWidth="389.0" pickOnBounds="true" preserveRatio="true" styleClass="Logo" GridPane.columnIndex="3" GridPane.columnSpan="2" GridPane.rowIndex="1" />
